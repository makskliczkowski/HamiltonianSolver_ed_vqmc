#!/bin/bash
#SBATCH -N1
#SBATCH --cpus-per-task=1
#SBATCH --mem-per-cpu=1
#SBATCH --time=1
#SBATCH --partition=long
#SBATCH --output=/home/kliczkowski/CODES/QuantumEigenSolver/cpp/library//SLURM/out-%j-qsm_mat_ns=1,.out
#SBATCH --job-name=qsm_mat_ns=1,
#SBATCH --error=/home/kliczkowski/CODES/QuantumEigenSolver/cpp/library//SLURM/err_qsm_mat_ns=1,_log-%A-%a.out

